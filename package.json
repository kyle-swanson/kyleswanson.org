{
  "name": "react-static-material-ui",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "react-static start",
    "build": "react-static build && cp -r wanagon dist/wanagon && cp -r projects dist/projects && cp -r images dist/images",
    "serve": "serve dist -p 3000",
    "fulldeploy": "yarn build && yarn deploy",
    "deploy": "git push origin :gh-pages && git subtree push --prefix dist origin gh-pages"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "material-ui": "^1.0.0-beta.37",
    "material-ui-icons": "^1.0.0-beta.36",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-router": "^4.2.0",
    "react-static": "latest",
    "typeface-roboto": "0.0.54"
  },
  "devDependencies": {
    "eslint-config-react-tools": "1.x.x",
    "serve": "^6.1.0"
  }
}
